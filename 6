#include <iostream>
#include <cmath>
using namespace std;

int main()
{ 
double x,y,a1,a2,b1,b2,c1,c2;
cout<<"A1= "; cin>>a1;
cout<<"A2= "; cin>>a2;
cout<<"B1= "; cin>>b1;
cout<<"B2= "; cin>>b2;
cout<<"C1= "; cin>>c1;
cout<<"C2= "; cin>>c2;
if ((b2*a1-a2*b1)!=0){y=(c2*a1-a2*c1)/(b2*a1-a2*b1);
x=(c1-b1*y)/a1;
cout<<"X= "<<x<<endl;
cout<<"Y= "<<y<<endl;}
else {cout<<"Решения нет"<<endl;}
return 0;
} 
